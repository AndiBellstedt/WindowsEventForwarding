                    <TableColumnHeader>
                        <Label>Locale</Label>
                    </TableColumnHeader>
                    <TableColumnHeader>
                        <Label>LogFile</Label>
                    </TableColumnHeader>
                </TableHeaders>
            
                <TableRowEntries>
                    <TableRowEntry>
                        <TableColumnItems>
                            <TableColumnItem> 
                                <PropertyName>ComputerName</PropertyName>
                            </TableColumnItem> 
                            <TableColumnItem> 
                                <PropertyName>Name</PropertyName>
                            </TableColumnItem> 
                            <TableColumnItem> 
                                <PropertyName>Type</PropertyName>
                            </TableColumnItem> 
                            <TableColumnItem> 
                                <PropertyName>Enabled</PropertyName>
                            </TableColumnItem> 
                            <TableColumnItem> 
