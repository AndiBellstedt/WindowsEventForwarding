                            </TableColumnItem> 
                            <TableColumnItem> 
                                <PropertyName>LogFile</PropertyName>
                            </TableColumnItem> 
                        </TableColumnItems> 
                    </TableRowEntry> 
                </TableRowEntries>
            </TableControl>
        </View>

